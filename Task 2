// Define the pin where the LED is connected
int ledPin = 13;  // LED is connected to digital pin 13

void setup() {
  // Initialize the digital pin as an output.
  pinMode(ledPin, OUTPUT);
}

void loop() {
  // Turn the LED on
  digitalWrite(ledPin, HIGH);
  
  // Wait for 1 second
  delay(1000);
  
  // Turn the LED off
  digitalWrite(ledPin, LOW);
  
  // Wait for 1 second
  delay(1000);
}
